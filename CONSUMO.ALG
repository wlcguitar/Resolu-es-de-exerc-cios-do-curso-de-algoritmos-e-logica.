Algoritmo "consumo"

Var

   total_per, combustivel_Gas, consumo_Medio : real


Inicio

   escreva(" Distancia percorrida : ")
   leia(total_per)
   escreva(" Combustivel gasto : " )
   leia(combustivel_Gas)
   
   consumo_medio <- total_per / combustivel_Gas
   
   escreva(" O consume medio e de : ", consumo_medio:4:3)


Fimalgoritmo